        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shirodemo.kay.mapper.RoleMapper">

    <resultMap id="BaseResultMap" type="com.shirodemo.kay.entity.Role">
            <id property="roleId" column="role_id" jdbcType="BIGINT"/>
